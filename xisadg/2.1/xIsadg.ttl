# ========== dateType ========== #
###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#dateType
xisadg:dateType a owl:DatatypeProperty ;
  rdfs:comment "CC (Century); YYYY-MM-DDThh:mm; YYYY-MM-DDThh:mm:ss; YYYY; YYYY-MM; YYYY-MM-DD; unknown" ;
  owl:oneOf ( xisadg:centuryType xisadg:dateTimeReducedType xsd:dateTime xsd:gYear xsd:gYearMonth xsd:date "unknown" ) ;
.
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#centuryType
  xisadg:centuryType a owl:DatatypeProperty ;
    rdfs:comment "CC (Century)" ;
    owl:withRestrictions ( [ xsd:pattern "^[0-9]{2}$"^^xsd:string ] ) ;
  .
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#dateTimeReducedType
  xisadg:dateTimeReducedType a owl:DatatypeProperty ;
    rdfs:comment "YYYY-MM-DDThh:mm" ;
    owl:withRestrictions ( [ xsd:pattern "^((000[1-9])|(00[1-9][0-9])|(0[1-9][0-9]{2})|([1-9][0-9]{3}))-((0[1-9])|(1[012]))-((0[1-9])|([12][0-9])|(3[01]))T(([01][0-9])|(2[0-3]))(:[0-5][0-9])$"^^xsd:string ] ) ;
  .

