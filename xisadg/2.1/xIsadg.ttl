@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xisadg: <https://github.com/KOST-CECO/ontologies/blob/master/xisadg#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

###########################################################################
#    Ontology                                                             #
###########################################################################

###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg
<https://github.com/KOST-CECO/ontologies/blob/master/xisadg> a owl:Ontology ;
  dct:creator "Koordinationsstelle für die dauerhafte Archivierung elektronischer Unterlagen (KOST)"@de ,
    "centre de coordination pour l'archivage à long terme de documents électroniques (CECO)"@fr ;
  dct:description "Ontologie für xIsadg (XML Schema basierend auf ISAD(G) - General International Standard Archival Description)"@de ,
    "Ontology for xIsadg (XML Schema based on ISAD(G) - General International Standard Archival Description)"@en ;
  dct:title "xIsadg Ontologie"@de ,
    "The xIsadg Ontology"@en ;
  owl:versionInfo "Created with rdfEditor, WebVOWL, TopBraid Composer & Protégé" ;
.

###########################################################################
#    Beschreibung der Verzeichnungseinheit                                #
###########################################################################

# ========== ArchivalDescription ========== #
###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#ArchivalDescription
xisadg:ArchivalDescription a rdfs:Class ; 
  rdfs:comment "Globales Element für die Beschreibung einer Verzeichnungseinheit"@de ,
    "global element for an ISADG-description of an description unit"@en ,
    "élement globale pour la description d'une unité de description"@fr ;
  .

  #---------- 1) hasIdentity ----------#
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasIdentity
  xisadg:hasIdentity a rdf:Property ;
    rdfs:comment "1) Area dell'identificazione"@it ,
      "1) Identifikation"@de ,
      "1) Zone d'identification"@fr ,
      "1) identity statement area"@en ;
    rdfs:domain xisadg:ArchivalDescription ;
    rdfs:range xisadg:Identity ;
  .
    ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#Identity
    xisadg:Identity a rdfs:Class ;
      rdfs:comment "1) Area dell'identificazione"@it ,
        "1) Identifikation"@de ,
        "1) Zone d'identification"@fr ,
        "1) identity statement area"@en ;
    .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasReferenceCode
      xisadg:hasReferenceCode a rdf:Property ;
        rdfs:comment "1.1) Segnatura/e o codice/i identificativo/i: Identificare univocamente l'unità di descrizione e stabilire un collegamento con la descrizione che la rappresenta"@it ,
          "1.1) Référence: identifier de manière unique l'unité de description et établir un lien avec la description qui la représente"@fr ,
          "1.1) Signatur(en): Identifizierung des Archivs und Verknüpfung des Archivguts mit der zugehörigen Verzeichnung"@de ,
          "1.1) Reference code(s): To identify uniquely the unit of description and to provide a link to the description that represents it"@en ;
        rdfs:domain xisadg:Identity ;
        rdfs:range xsd:anyURI ; 
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasTitle
      xisadg:hasTitle a rdf:Property ;
        rdfs:comment "1.2) Denominazione o titolo: Denominare l'unità di descrizione"@it ,
          "1.2) Intitulé / analyse: Dénommer l'unité de description"@fr ,
          "1.2) Titel: Benennung der Verzeichnungseinheit"@de ,
          "1.2) Title: To name the unit of description"@en ;
        rdfs:domain xisadg:Identity ;
        rdfs:range xsd:string ; 
        rdfs:subPropertyOf dct:title ;
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasDateRange
      xisadg:hasDateRange a rdf:Property ;
        rdfs:comment "1.3) Data/e: Identificare e segnalare la/e data/e dell'unità di descrizione"@it ,
          "1.3) Date(s): To identify and record the date(s) of the unit of description"@en ,
          "1.3) Date: Identifier et donner la (les) date(s) de lunité de description"@fr ,
          "1.3) Entstehungszeitraum / Laufzeit: Identifizierung des Entstehungsdatums bzw. -zeitraums der in der Verzeichnungseinheit enthaltenen Unterlagen"@de ;
        rdfs:domain xisadg:Identity ;
        rdfs:range xisadg:DateRange ; 
        rdfs:subPropertyOf dct:date, dct:created ;
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasDatePoint
      xisadg:hasDatePoint a rdf:Property ;
        rdfs:comment "1.3) Data/e: Identificare e segnalare la/e data/e dell'unità di descrizione"@it ,
          "1.3) Date(s): To identify and record the date(s) of the unit of description"@en ,
          "1.3) Date: Identifier et donner la (les) date(s) de lunité de description"@fr ,
          "1.3) Entstehungszeitraum / Laufzeit: Identifizierung des Entstehungsdatums bzw. -zeitraums der in der Verzeichnungseinheit enthaltenen Unterlagen"@de ;
        rdfs:domain xisadg:Identity ;
        rdfs:range xisadg:dateType ; 
        rdfs:subPropertyOf dct:created ;
      .

  #---------- 2) Context ----------#
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasContext
  xisadg:hasContext a rdf:Property ;
    rdfs:comment "Area delle informazioni sul contesto"@it ,
      "2) Kontext"@de ,
      "2) Zone du contexte"@fr ,
      "2) context area"@en ;
    rdfs:domain xisadg:ArchivalDescription ;
    rdfs:range xisadg:Context ;
  .
    ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#Context
    xisadg:Context a rdfs:Class ;
      rdfs:comment "Area delle informazioni sul contesto"@it ,
        "2) Kontext"@de ,
        "2) Zone du contexte"@fr ,
        "2) context area"@en ;
    .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasCreator
      xisadg:hasCreator a rdf:Property ;
        rdfs:comment "2.1) Denominazione del/dei soggetto/i produttore/i: Identificare il soggetto (o i soggetti) produttori dell'unità di descrizione"@it ,
          "2.1) Name der Provenienzstelle: Identifizierung der Provenienzstelle(n), bei der (denen) die Verzeichnungseinheit entstanden ist"@de ,
          "2.1) Name of creator(s): To identify the creator (or creators) of the unit of description"@en ,
          "2.1) Nom du producteur: Identifier le ou les producteurs de l'unité de description"@fr ;
        rdfs:domain xisadg:Context ;
        rdfs:range xisadg:Agent ; 
        rdfs:subPropertyOf dct:creator ;
      .
        ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#Agent
        xisadg:Agent a rdfs:Class ;
          rdfs:comment "Agent include person, organization, and software"@en ;
          rdfs:subClassOf dct:Agent ;
        .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentName
          xisadg:hasAgentName a rdf:Property ;
            rdfs:comment "Agent Name"@de ;
            rdfs:domain xisadg:Agent ; 
            rdfs:range xsd:string ; 
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentId
          xisadg:hasAgentId a rdf:Property ;
            rdfs:comment "Agent ID"@de ;
            rdfs:domain xisadg:Agent ; 
            rdfs:range xsd:anyURI ; 
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentCv
          xisadg:hasAgentCv a rdf:Property ;
            rdfs:comment "Agent CV"@de ;
            rdfs:domain xisadg:Agent ; 
            rdfs:range xisadg:AgentCv ; 
          .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAdminBioHistory
      xisadg:hasAdminBioHistory a rdf:Property ;
        rdfs:comment "2.2) Stori istituzionale/amministrativa, nota biografica: Fornire una storia istituzionale/amministrativa o un profilo biografico del soggetto produttore (o i soggetti produttori), per collocare la documentazione nel proprio contesto e facilitarne la comprensione"@it ,
          "2.2) Verwaltungsgeschichte/Biographische Angaben: Information über Verwaltungsgeschichte der Provenienzstelle bzw. die Biographie, wenn es sich um natürliche Personen handelt, zum besseren Verständnis des zur Verzeichnungseinheit gehörenden Kontextes"@de ,
          "2.2) Administrative / Biographical history: To provide an administrative history of, or biographical details on, the creator (or creators) of the unit of description to place the material in context and make it better understood"@en ,
          "2.2) Histoire administrative / Notice biographique: Fournir un historique ou une biographie de ou des producteurs de l'unité de description afin de situer les documents dans leur contexte et de les rendre plus intelligibles"@fr ;
        rdfs:domain xisadg:Context ;
        rdfs:range xisadg:AgentCv ; 
      .
        ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#AgentCv
        xisadg:AgentCv a rdfs:Class ;
          rdfs:comment "Verwaltungsgeschichte/Biographische Angaben der Provenienzstelle"@de ;
        .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentCvDatesComment
          xisadg:hasAgentCvDatesComment a rdf:Property ;
            rdfs:comment "Comment to Date"@en ;
            rdfs:domain xisadg:AgentCv ;
            rdfs:range xsd:string ;
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentCvDateRange
          xisadg:hasAgentCvDateRange a rdf:Property ;
            rdfs:comment "Data/e"@it ,
              "Date(s)"@en ,
              "Date"@fr ,
              "Datum"@de ;
            rdfs:domain xisadg:AgentCv ; 
            rdfs:range xisadg:DateRange ; 
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentCvDatePoint
          xisadg:hasAgentCvDatePoint a rdf:Property ;
            rdfs:comment "Data/e"@it ,
              "Date(s)"@en ,
              "Date"@fr ,
              "Datum"@de ;
            rdfs:domain xisadg:AgentCv ; 
            rdfs:range xisadg:dateType ; 
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAgentCvId
          xisadg:hasAgentCvId a rdf:Property ;
            rdfs:comment "CV ID"@en ;
            rdfs:domain xisadg:AgentCv ;
            rdfs:range xsd:anyURI ;
          .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasArchivalHistory
      xisadg:hasArchivalHistory a rdf:Property ;
        rdfs:comment "2.3) Storia archivistica: Fornire le informazioni sulla storia dell'unità di descrizione che sono significative ai fini dell'autenticità, integrità e interpretazione della documentazione"@it ,
          "2.3) Bestandesgeschichte: Information über den Wechsel der Eigentums- und Besitzverhältnisse der Verzeichnungseinheit, die für deren Authentizität, Integrität, Vollständigkeit und Interpretation von wesentlicher Bedeutung sind"@de ,
          "2.3) Archival history: To provide information on the unit of description that is significant for its authenticity, integrity and interpretation"@en ,
          "2.3) Historique de la conservation: Fournir sur l'histoire de l'nité de description les renseignements importants qui contribuent à son interprétation et qui prouvent son authenticité et sont intégrité"@fr ;
        rdfs:domain xisadg:Context ;
        rdfs:range xsd:string ; 
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAcqInfo
      xisadg:hasAcqInfo a rdf:Property ;
        rdfs:comment "2.4) Modalità di acquisizione o versamento: Identificare il soggetto dal quale è stata acquisita o che ha versato la documentazione"@it ,
          "2.4) Abgebende Stelle: Darstellung der Umstände, die mit der direkten Übernahme der Verzeichnungseinheit von der abgebenden Stelle verbunden sind"@de ,
          "2.4) Immediate source of acquisition or transfer: To identify the immediate source of acquisition or transfer"@en ,
          "2.4) Modalités d'entrée: Identifier les circonstances de l'entrée dans les archives"@fr ;
        rdfs:domain xisadg:Context ;
        rdfs:range xisadg:Agent ; 
      .

  #---------- 3) hasContentStructure ----------#
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasContentStructure
  xisadg:hasContentStructure a rdf:Property ;
    rdfs:comment "3) Area delle informazioni relative al contenuto e alla struttura"@it ,
      "3) Inhalt und innere Ordnung"@de ,
      "3) Zone du contenu et de la structure"@fr ,
      "3) content and structure area"@en ;
    rdfs:domain xisadg:ArchivalDescription ;
    rdfs:range xisadg:ContentStructure ;
  .
    ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#ContentStructure
    xisadg:ContentStructure a rdfs:Class ;
      rdfs:comment "3) Area delle informazioni relative al contenuto e alla struttura"@it ,
        "3) Inhalt und innere Ordnung"@de ,
        "3) Zone du contenu et de la structure"@fr ,
        "3) content and structure area"@en ;
    .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAppraisalDestruction
      xisadg:hasAppraisalDestruction a rdf:Property ;
        rdfs:comment "3.2) Procedure, tempi e criteri di valutazione e scarto: Fornire le informazioni relative a tutte le operazioni e di valutazione e di scarto e alle relative epoche"@it ,
          "3.2) Bewertung und Kassation: Bereitstellung von Informationen über jede vorgenommene Bewertung und Kassation"@de ,
          "3.2) Appraisal, destruction and scheduling information: To provide information on any appraisal, destruction and scheduling action"@en ,
          "3.2) Evaluation, tris et éliminations, sort final: Fournir des resneignements sur l'évaluation, les oprérations detri ou d'élimination et le sort final des documents"@fr ;
        rdfs:domain xisadg:ContentStructure ;
        rdfs:range xsd:string ; 
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAccruals
      xisadg:hasAccruals a rdf:Property ;
        rdfs:comment "3.3) Incrementi previsti: Informare l'utente su previsti accrescimenti dell'unità di descrizione"@it ,
          "3.3) Neuzugänge: Angaben für den Benutzer über mögliche Veränderungen im Umfang der Verzeichnungseinheit"@de ,
          "3.3) Accruals: To inform the user of foreseen additions to the unit of description"@en ,
          "3.3) Accroissements: Informer l'usager de possibles entrées complétant l'unité de description"@fr ;
        rdfs:domain xisadg:ContentStructure ;
        rdfs:range xsd:string ; 
      .
      ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasArrangement
      xisadg:hasArrangement a rdf:Property ;
        rdfs:comment "3.4) Criteri di ordinamento: Fornire informazioni sulla struttura interna, l'organizzazione e/o il sistema di classificazione dell'unità di descrizione"@it ,
          "3.4) Ordnung und Klassifikation: Bereitstellung von Informationen über die Ordnung und Klassifikation der Verzeichnungseinheit"@de ,
          "3.4) System of arrangement: To provide information on the internal structure, the order and/or the system of classification of the unit of description"@en ,
          "3.4) Mode de classement: Fournir des renseignements sur la structure, l'ordre ou le plan de classement de l'unité de description"@fr ;
        rdfs:domain xisadg:ContentStructure ;
        rdfs:range xisadg:Arrangement ; 
      .
        ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#Arrangement
        xisadg:Arrangement a rdfs:Class ;
          rdfs:comment "3.4) Criteri di ordinamento: Fornire informazioni sulla struttura interna, l'organizzazione e/o il sistema di classificazione dell'unità di descrizione"@it ,
            "3.4) Ordnung und Klassifikation: Bereitstellung von Informationen über die Ordnung und Klassifikation der Verzeichnungseinheit"@de ,
            "3.4) System of arrangement: To provide information on the internal structure, the order and/or the system of classification of the unit of description"@en ,
            "3.4) Mode de classement: Fournir des renseignements sur la structure, l'ordre ou le plan de classement de l'unité de description"@fr ;
        .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasArrangementComment
          xisadg:hasArrangementComment a rdf:Property ;
            rdfs:comment "Comment"@en ;
            rdfs:domain xisadg:Arrangement ;
            rdfs:range xsd:string ;
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasArrangementDateRange
          xisadg:hasArrangementDateRange a rdf:Property ;
            rdfs:comment "Data/e"@it ,
              "Date(s)"@en ,
              "Date"@fr ,
              "Datum"@de ;
            rdfs:domain xisadg:Arrangement ; 
            rdfs:range xisadg:DateRange ; 
          .
          ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasArrangementDatePoint
          xisadg:hasArrangementDatePoint a rdf:Property ;
            rdfs:comment "Data/e"@it ,
              "Date(s)"@en ,
              "Date"@fr ,
              "Datum"@de ;
            rdfs:domain xisadg:Arrangement ; 
            rdfs:range xisadg:dateType ; 
          .

  #---------- 4) ConditionsAccessUse ----------#
  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasConditionsAccessUse
  xisadg:hasConditionsAccessUse a rdf:Property ;
    rdfs:comment "4) Area delle informazioni relative alle condizioni di accesso ed utilizzazione"@it ,
      "4) Zugangs- und Benutzungsbestimmungen"@de ,
      "4) Zone des conditions d'accès et d'utilisation"@fr ,
      "4) conditions of access and use area"@en ;
    rdfs:domain xisadg:ArchivalDescription ;
    rdfs:range xisadg:ConditionsAccessUse ;
  .
    ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#ConditionsAccessUse
    xisadg:ConditionsAccessUse a rdfs:Class ;
      rdfs:comment "4) Area delle informazioni relative alle condizioni di accesso ed utilizzazione"@it ,
        "4) Zugangs- und Benutzungsbestimmungen"@de ,
        "4) Zone des conditions d'accès et d'utilisation"@fr ,
        "4) conditions of access and use area"@en ;
    .
	  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAccessConditions
	  xisadg:hasAccessConditions a rdf:Property ;
		rdfs:comment "4.1) Condizioni che regolano l'accesso: Fornire informazioni sulla condizione giuridica e su altre forme di regolamentazione che limitano o condizionano l'acesso all'unità di descrizione"@it ,
		  "4.1) Zugangsbestimmungen: Angabe derjenigen Bestimmungen, die den Zugang zur Verzeichnungseinheit einschränken oder beeinflussen"@de ,
		  "4.1) Conditions governing access: To provide information on the legal status or other regulations that restrict or affect access to the unit of description"@en ,
		  "4.1) Conditions d'accès: Fournir des renseignements sur le statut juridique et sur les autres dispositions de toute nature qui restreignent ou affectent l'accès à l'unité des descriptions"@fr ;
		rdfs:domain xisadg:ConditionsAccessUse ;
		rdfs:range xisadg:AccessConditions ; 
		rdfs:subPropertyOf dct:accessRights ;
	  .
		###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#AccessConditions
		xisadg:AccessConditions a rdfs:Class ;
		  rdfs:comment "4.1) Condizioni che regolano l'accesso: Fornire informazioni sulla condizione giuridica e su altre forme di regolamentazione che limitano o condizionano l'acesso all'unità di descrizione"@it ,
			"4.1) Zugangsbestimmungen: Angabe derjenigen Bestimmungen, die den Zugang zur Verzeichnungseinheit einschränken oder beeinflussen"@de ,
			"4.1) Conditions governing access: To provide information on the legal status or other regulations that restrict or affect access to the unit of description"@en ,
			"4.1) Conditions d'accès: Fournir des renseignements sur le statut juridique et sur les autres dispositions de toute nature qui restreignent ou affectent l'accès à l'unité des descriptions"@fr ;
		  rdfs:subClassOf dct:RightsStatement ;
		.
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasPrivacyProtection
		  xisadg:hasPrivacyProtection a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Datenschutz (Angabe, ob in der Verzeichnungseinheit besonders schützenswerte Personendaten vorhanden sind oder nicht)"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:boolean ; 
		  .
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasOpenToThePublic
		  xisadg:hasOpenToThePublic a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Öffentlichkeitsstatus (Angabe über die Zugänglichkeit der Verzeichnungseinheit gemäss den geltenden Informationsfreiheits- resp. Öffentlichkeitsgesetzen)"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:string ;
			# SHACL sh:pattern "^undefined|public|not_public$" ;
		  .
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasClassification
		  xisadg:hasClassification a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Klassifizierungskategorie (Angabe zur Geheimhaltung/Klassifikation der Verzeichnungseinheit)"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:string ; 
			# SHACL sh:pattern "^unclassified|confidential|secret|in_house|other$" ;
		  .
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasOtherAccessConditions
		  xisadg:hasOtherAccessConditions a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Weitere Zugangs- und Nutzungsbedingungen (Angabe über weitere Zugangs- und Nutzungsbedingungen; NICHT Repro-Bestimmungen)"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:string ; 
		  .
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasAccessConditionsNotes
		  xisadg:hasAccessConditionsNotes a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Bemerkungen zu Zugangs- und Nutzungsbedingungen"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:string ; 
		  .
		  ###  https://github.com/KOST-CECO/ontologies/blob/master/xisadg#hasRetentionPeriod
		  xisadg:hasRetentionPeriod a rdf:Property ;
			rdfs:comment "Condizioni che regolano l'accesso: "@it ,
			  "Zugangsbestimmungen: Schutzfrist (Dauer der Schutzfrist in Jahren)"@de ,
			  "Conditions governing access: "@en ,
			  "Conditions d'accès: "@fr ;
			rdfs:domain xisadg:AccessConditions ;
			rdfs:range xsd:integer ; 
		  .
